{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node scripts/generate-posts-list.js && vite",
    "dev:playwright": "node scripts/generate-posts-list.js && dotenv -e .env.contract -e .env.local -- vite --host 127.0.0.1",
    "prebuild": "node scripts/generate-posts-list.js",
    "build": "dotenv -e .env.local -e .env.contract -- vite build",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "cross-env COVERAGE=true vitest run --coverage",
    "test:watch": "vitest watch",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:contract": "vitest run --config vitest.contract.config.js",
    "test:contract:watch": "vitest --config vitest.contract.config.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:legacy": "node --test tests/legacy/apiError.test.mjs",
    "lint": "eslint .",
    "preview": "dotenv -e .env.local -e .env.contract -- vite preview --host 127.0.0.1",
    "playwright:open": "dotenv -e .env.contract -e .env.local -- concurrently --kill-others --success first \"npm run dev:playwright\" \"npx playwright open http://127.0.0.1:3000 --browser=chromium\""
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.11.2",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "aws-amplify": "^6.15.4",
    "chart.js": "^4.5.0",
    "framer-motion": "^12.23.12",
    "joi": "^18.0.1",
    "lucide-react": "^0.539.0",
    "pitchy": "^4.1.0",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.12.0",
    "recharts": "^3.1.2",
    "remark-gfm": "^4.0.1",
    "rubberband-wasm": "^3.3.0",
    "soundfont-player": "^0.12.0",
    "uuid": "^11.1.0",
    "vite-plugin-wasm": "^3.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.56.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.7.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "vite": "^7.1.9",
    "vitest": "^3.2.4",
    "whatwg-fetch": "^3.6.20"
  }
}
